{
    "name": "Browser-data",
    "description": "Get Browser data like tab and window counts, history and memory",
    "manifest_version": 3,
    "version": "0.1.2",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "icons/browser.png",
            "48": "icons/browser.png"
        }, 
        "default_popup": "popup.html"
    },
    "permissions": ["bookmarks", "history", "storage"]
}